{
  "include": [
    "./validateRuntimeConfig.ts",
  ],
  "compilerOptions": {
    "paths": {
      "~~/*": ["../*"],
    },
    "target": "ES2023",
    "lib": ["ES2023"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "types": ["node"],
    "rootDir": "../",
    "outDir": "../.output",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
